{
  "name": "stylus",
  "description": "Robust, expressive, and feature-rich CSS superset",
  "version": "0.45.1",
  "author": "TJ Holowaychuk <tj@vision-media.ca>",
  "keywords": [
    "css",
    "parser",
    "style",
    "stylesheets",
    "jade",
    "language"
  ],
  "repository": "git://github.com/LearnBoost/stylus",
  "main": "./index.js",
  "browserify": "./lib/browserify.js",
  "engines": {
    "node": "*"
  },
  "bin": {
    "stylus": "./bin/stylus"
  },
  "scripts": {
